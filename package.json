{
  "homepage": "https://dexignare.github.io/dexignare-fontend",
  "name": "tailwind",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.2",
    "env-cmd": "^10.1.0",
    "flowbite": "^1.3.2",
    "postcss": "^8.4.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "start:dev": "env-cmd -f .env.dev npm run start",
    "start:staging": "env-cmd -f .env.staging npm run start",
    "start:prod": "env-cmd -f .env.prod npm run start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-serve": "serve -s build",
    "build:dev": "env-cmd -f .env.dev npm run build",
    "build:qa": "env-cmd -f .env.qa npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:uat": "env-cmd -f .env.uat npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "tailwindcss": "^3.0.18"
  }
}
